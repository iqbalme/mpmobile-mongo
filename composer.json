{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "berkayk/onesignal-laravel": "^1.0",
        "fabiang/xmpp": "^0.7.0",
        "flipbox/lumen-generator": "^5.6",
        "illuminate/redis": "^5.6",
        "ixudra/curl": "^6.16",
        "jenssegers/mongodb": "3.5.*",
        "jenssegers/mongodb-session": "^1.2",
        "laravel/lumen-framework": "5.8.*",
        "laravelista/lumen-vendor-publish": "^2.1",
        "nathanmac/parser": "^4.3",
        "nyholm/psr7": "^1.1",
        "predis/predis": "^1.1",
        "spatie/array-to-xml": "^2.7",
        "spomky-labs/base64url": "2.0.0",
        "tymon/jwt-auth": "^1.0.0-rc.2",
        "vlucas/phpdotenv": "^3.3",
        "wn/lumen-generators": "^1.3"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "phpunit/phpunit": "^7.0",
        "mockery/mockery": "^1.0",
        "niellles/lumen-commands": "dev-master"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
